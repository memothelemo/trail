{
  "process": [
    {
      "rule": "convert_require",
      "target": {
        "name": "roblox",
        "rojo_sourcemap": "sourcemap.json",
        "indexing_style": "wait_for_child"
      },
      "current": {
        "name": "path",
        "sources": {
          "@DevPackages": "DevPackages/",
          "@Modules": "out/library/",
          "@Trail": "modules/trail/out/",
          "@TrailSubscriber": "modules/trail-subscriber/out/"
        }
      }
    },
    {
      "rule": "inject_global_value",
      "identifier": "__TRAIL_UNIT_TESTS__",
      "value": false
    },
    {
      "rule": "inject_global_value",
      "identifier": "__TRAIL_RUN_BENCHMARKS__",
      "value": true
    },
    "compute_expression",
    "remove_unused_if_branch",
    "remove_unused_while",
    "filter_after_early_return",
    "remove_nil_declaration",
    "remove_empty_do"
  ]
}